    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write './src/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
